{
  "name": "kiss-arch",
  "version": "0.10.0",
  "main": "index.js",
  "typings": "dist/src/index.d.ts",
  "description": "Web app architecture kept simple, stupid",
  "keywords": [
    "kiss",
    "architecture",
    "web",
    "app",
    "typescript",
    "browser",
    "Node.js"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "esbuild src/index.ts --format=cjs --bundle --minify --target=chrome58 --outfile=index.js",
    "postbuild": "tsc",
    "predeploy": "yarn test",
    "deploy": "npm pack",
    "pretest": "yarn build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kyr0/kiss-arch.git"
  },
  "bugs": {
    "url": "https://github.com/kyr0/kiss-arch/issues"
  },
  "author": "Aron Homberg <info@aron-homberg.de>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.7",
    "esbuild": "^0.14.3",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  }
}
